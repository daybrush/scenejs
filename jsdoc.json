{
    "plugins": [],
	"recurseDepth": 10,
	"opts": {
		"template": "./node_modules/daybrush-jsdoc-template",
		"destination": "./doc/"
    },
    "source": {
		"include": ["./src", "README.md"], 
        "includePattern": "(.+\\.js(doc|x)?|.+\\.ts(doc|x)?)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}